
public class Runner {
	public static void main(String[] args) {
		
		printFromTo(1,10);
	}
	
	public static void printIntFromTo(int start, int stop) {
		if(start < stop) {
			for(int i = start; i <= stop; i++) {
				System.out.println(i);
			}
		} else {
			for(int i = stop; i <= start; i++) {
				System.out.println(i);
			}
		}
	}

}
